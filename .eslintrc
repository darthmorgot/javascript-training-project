{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "comma-dangle": "off",
    "eol-last": "off",
    "arrow-parens": [2, "as-needed", {"requireForBlockBody": true}],
    "require-jsdoc": "off"
  },
  "extends": ["eslint:recommended", "google"]
}